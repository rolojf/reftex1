{
  "name": "elm-pages-app",
  "scripts": {
    "postinstall": "elm-tooling install",
    "elm": "elm-pages dev",
    "build": "tailwindcss -o public/tailwind.css --minify && elm-pages build",
    "tail": "tailwindcss -o public/tailwind.css",
    "dev": "concurrently \"elm-pages dev\" \"tailwindcss -o public/tailwind.css --watch \""
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.0",
    "@tailwindcss/typography": "^0.5.0",
    "autoprefixer": "^10.4.1",
    "concurrently": "6.5.1",
    "elm-optimize-level-2": "0.2.3",
    "elm-pages": "2.1.11",
    "elm-review": "2.6.1",
    "elm-tooling": "1.7.0",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.9"
  }
}
